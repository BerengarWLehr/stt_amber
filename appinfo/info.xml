<?xml version="1.0"?>
<info>
	<id>stt_amber</id>
	<name>Transcription</name>
	<summary>Transcribe audiofiles via AmberScript</summary>
	<description>
	<![CDATA[
Transcribe files via the context menu by sending them to amber script and later uploading the transcription result.
	]]></description>
	<version>0.1.0</version>
	<licence>MPL 2.0</licence>
	<author mail="berengar.lehr@uni-jena.de" homepage="https://www.uni-jena.de/10680/multimediazentrum">Berengar Lehr</author>
	<namespace>PyAppV1_stt_amber</namespace>
	<category>tools</category>
	<website>https://git.uni-jena.de/rz/mmz/nextcloud2amberscript</website>
	<bugs>https://git.uni-jena.de/rz/mmz/nextcloud2amberscript/-/issues</bugs>
	<repository type="git">git@git.uni-jena.de:rz/mmz/nextcloud2amberscript.git</repository>
	<dependencies>
		<nextcloud min-version="29" max-version="30"/>
	</dependencies>
	<external-app>
		<docker-install>
			<registry>git.uni-jena.de:5050</registry>
			<image>rz/mmz/nextcloud2amberscript</image>
			<image-tag>latest</image-tag>
		</docker-install>
	</external-app>
</info>
